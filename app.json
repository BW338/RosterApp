{
  "expo": {
    "name": "RosterApp",
    "slug": "RosterApp",
    "version": "1.3.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "usesNonExemptEncryption": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.guido1988.TP",
      "buildNumber": "14",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Esta aplicaci√≥n necesita acceso a tu calendario para poder agregar tus eventos de vuelo y actividades.",
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "PDF Document",
            "LSItemContentTypes": [
              "com.adobe.pdf"
            ],
            "LSHandlerRank": "Alternate"
          },
          {
            "CFBundleTypeName": "PDF Document",
            "LSItemContentTypes": [
              "com.adobe.pdf"
            ],
            "LSHandlerRank": "Alternate"
          },
          {
            "CFBundleTypeName": "PDF Document",
            "LSItemContentTypes": [
              "com.adobe.pdf"
            ],
            "LSHandlerRank": "Alternate"
          },
          {
            "CFBundleTypeName": "PDF Document",
            "LSItemContentTypes": [
              "com.adobe.pdf"
            ],
            "LSHandlerRank": "Alternate"
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "BILLING"
      ],
      "versionCode": 27,
      "package": "com.github1988.rosterapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "application/pdf"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ed3a9d47-e110-44fe-a969-2fc171973a16"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
